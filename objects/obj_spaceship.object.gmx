            <string>x = mouse_x;
y = mouse_y;

if(!slowing &amp;&amp; keyboard_check_pressed(vk_space) &amp;&amp; energy - energy_consumption &gt; 0){
    slowing = true;
    energy = energy - energy_consumption;
    
    var i;
    for(i = 0; i &lt; instance_number(obj_moving); i++){
        var ii;
        ii = instance_find(obj_moving, i);
        ii.speed_mod = energy_slow;
    }
    background_vspeed[0] = bk_normal_speed * energy_slow;
}
else if(slowing){
    if(energy &lt;= 0 || !keyboard_check(vk_space)){
        if(energy &lt;= 0){
            energy = 0;
            slowing = false;
        
            var i;
            for(i = 0; i &lt; instance_number(obj_moving); i++){
                var ii;
                ii = instance_find(obj_moving, i);
                ii.speed_mod = 1;
            }
            
            background_vspeed[0] = bk_normal_speed;
        }
    }
    else{
        energy = energy - energy_consumption;
    }
}

if(!slowing){
    energy = min(energy + energy_recovery, energy_max);
}
</string>
